{
	"name": "laravel/laravel",
	"description": "The Laravel Framework.",
	"keywords": ["framework", "laravel"],
	"license": "MIT",
	"type": "project",
	"require": {
		"php": ">=5.6.4",
		"authorizenet/authorizenet": "^1.9",
		"cviebrock/eloquent-sluggable": "^4.2",
		"cviebrock/eloquent-taggable": "^2.1",
		"doctrine/dbal": "^2.5",
		"guzzlehttp/guzzle": "^6.2",
		"htmlmin/htmlmin": "^5.6",
		"jrm2k6/cloudder": "0.4.*",
		"laravel/dusk": "^1.1",
		"laravel/framework": "5.4.*",
		"laravel/passport": "^2.0",
		"laravel/tinker": "^1.0",
		"lukepolo/laracart": "^1.3",
		"orangehill/iseed": "dev-master",
		"spatie/laravel-cookie-consent": "^1.0.0",
		"spatie/laravel-feed": "1.*",
		"spatie/laravel-sitemap": "2.*",
		"spatie/laravel-newsletter": "~3.7",
		"tcg/voyager": "^0.11.12",
		"torann/laravel-meta-tags": "^3.0",
		"webpatser/laravel-uuid": "^2.0"
	},
	"require-dev": {
		"fzaninotto/faker": "~1.4",
		"laravel/browser-kit-testing": "^1.0",
		"laravel/homestead": "^5.0",
		"mockery/mockery": "0.9.*",
		"phpunit/phpunit": "~5.7"
	},
	"autoload": {
		"classmap": ["database"],
		"psr-4": {
			"App\\": "app/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Tests\\": "tests/"
		},
		"classmap": ["tests/TestCase.php"]
	},
	"scripts": {
		"post-root-package-install": ["php -r \"file_exists('.env') || copy('.env.example', '.env');\""],
		"post-create-project-cmd": ["php artisan key:generate"],
		"post-install-cmd": ["Illuminate\\Foundation\\ComposerScripts::postInstall", "php artisan optimize"],
		"post-update-cmd": ["Illuminate\\Foundation\\ComposerScripts::postUpdate", "php artisan optimize"]
	},
	"config": {
		"preferred-install": "dist",
		"sort-packages": true,
		"bin-dir": "vendor/bin"
	}
}